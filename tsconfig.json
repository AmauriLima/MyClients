{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@services/*": ["./src/services/*"],
      "@utils/*": ["./src/utils/*"],
      "@components/*": ["./src/components/*"],
    },
  },
  "include": ["src"],
}
